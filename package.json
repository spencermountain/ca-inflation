{
  "name": "ca-inflation",
  "version": "0.0.1",
  "description": "calculate the inflation rate of the Canadian dollar",
  "main": "src/index.js",
  "unpkg": "builds/ca-inflation.min.js",
  "types": "types/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./types/index.d.ts",
        "default": "./builds/ca-inflation.mjs"
      },
      "require": {
        "types": "./types/index.d.cts",
        "default": "./builds/ca-inflation.cjs"
      }
    }
  },
  "scripts": {
    "build": "npm run pack && npm run version && rollup -c --silent",
    "version": "node ./scripts/version.js",
    "watch": "node --watch ./scratch.js",
    "test": "tape ./test/**/*.test.js | tap-dancer  --color always",
    "testb": "TESTENV=prod tape ./test/**/*.test.js | tap-dancer  --color always",
    "lint": "eslint ./src",
    "update": "node ./data/update.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/spencermountain/ca-inflation.git"
  },
  "files": [
    "src",
    "builds",
    "types"
  ],
  "prettier": {
    "trailingComma": "none",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 100
  },
  "devDependencies": {
    "rollup": "4.40.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-terser": "7.0.2",
    "tap-dancer": "0.3.4",
    "tape": "5.9.0"
  },
  "license": "MIT"
}